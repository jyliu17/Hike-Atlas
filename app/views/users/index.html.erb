<h1>Welcome Page</h1>
<p>Welcome to Hike Atlas! Please log in or sign up.</p>
<p>link to sign up</p>

<h1>User Login</h1>

<% if flash[:error] %>
  <p><%= flash[:error] %></p>
<% end %>

<%= form_for :session do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email  %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log In" %>
<% end %>